{
    "manifest_version": 3,
    "name": "Auto Refresh Extension",
    "version": "1.0",
    "description": "A simple auto-refresh extension",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_icon": {
        "16": "icons/chegg_16.png",
        "48": "icons/chegg_48.png",
        "128": "icons/chegg_128.png"
      },
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icons/chegg_16.png",
      "48": "icons/chegg_48.png",
      "128": "icons/chegg_128.png"
    },
    "background": {
        "service_worker": "background.js"
    }
      
  }
  